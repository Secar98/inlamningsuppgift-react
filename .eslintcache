            <tr key={index}>\r\n                    <td>{item[0]}</td>\r\n                    <td>{item[1]}</td>\r\n                  </tr>\r\n                );\r\n              })}\r\n              <tr>\r\n                <td>Email</td>\r\n                <td>\r\n                  <a href={`mailto:${customerItem.email}`}>\r\n                    {customerItem.email}\r\n                  </a>\r\n                </td>\r\n              </tr>\r\n              <tr>\r\n                <td>Website</td>\r\n                <td>\r\n                  <a href={customerItem.website} target=\"_blank\">\r\n                    {customerItem.website}\r\n                  </a>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n          <StyledNewButton onClick={deleteCustomer}>\r\n            Delete Customer\r\n          </StyledNewButton>\r\n          <Link to={`/home/${customerId}/edit`}>\r\n            <StyledButton>Edit Customer</StyledButton>\r\n          </Link>\r\n        </div>\r\n      ) : (\r\n        <span>Laddar data...</span>\r\n      )}\r\n    </StyledDetailDiv>\r\n  );\r\n}\r\n","C:\\Users\\Sebas\\Desktop\\inlamningsuppgift-react\\src\\pages\\CustomerUpdatePage.jsx",["60"],"C:\\Users\\Sebas\\Desktop\\inlamningsuppgift-react\\src\\pages\\CustomerCreatePage.jsx",[],"C:\\Users\\Sebas\\Desktop\\inlamningsuppgift-react\\src\\components\\StyledElements.jsx",[],"C:\\Users\\Sebas\\Desktop\\inlamningsuppgift-react\\src\\context\\UserContext.js",[],"C:\\Users\\Sebas\\Desktop\\inlamningsuppgift-react\\src\\context\\CustomerListContext.js",[],{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","severity":1,"message":"66","line":20,"column":6,"nodeType":"67","endLine":20,"endColumn":8,"suggestions":"68"},{"ruleId":"65","severity":1,"message":"69","line":15,"column":6,"nodeType":"67","endLine":15,"endColumn":8,"suggestions":"70"},{"ruleId":"65","severity":1,"message":"71","line":39,"column":6,"nodeType":"