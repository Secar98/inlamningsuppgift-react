nents/StyledElements';\r\nimport { UserContext } from './context/UserContext';\r\nimport CustomerCreatePage from './pages/CustomerCreatePage';\r\nimport CustomerDetailPage from './pages/CustomerDetailPage';\r\nimport CustomerListPage from './pages/CustomerListPage'\r\nimport CustomerUpdatePage from './pages/CustomerUpdatePage';\r\nimport LoginPage from './pages/LoginPage'\r\n\r\n\r\nfunction App() {\r\n  const history = useHistory()\r\n  const [token, setToken] = useState()\r\n\r\n\r\n  useEffect(() => {\r\n    userLoggedin();\r\n  }, [])\r\n\r\n  function userLoggedin() {\r\n    if (localStorage.getItem(\"WEBB20\")) {\r\n      history.push('/home')\r\n      setToken(true)\r\n    } else {\r\n      history.push('/login')\r\n    }\r\n  }\r\n\r\n  return (\r\n    <div>\r\n      <UserContext.Provider value={{ token, setToken }}>\r\n        {token ?\r\n          <StyledNavUl>\r\n            <StyledNavLi>\r\n              <Link to=\"/home\">Home</Link>\r\n            </StyledNavLi>\r\n            <StyledNavLi>\r\n              <Link to=\"/home/create\">Create Customer</Link>\r\n            </StyledNavLi>\r\n          </StyledNavUl>\r\n          : <div />\r\n        }\r\n\r\n        <Switch>\r\n          <Route path=\"/login\">\r\n            <LoginPage />\r\n          </Route>\r\n\r\n          <Route path=\"/home/create\">\r\n            <CustomerCreatePage />\r\n          </Route>\r\n\r\n          <Route\r\n            path=\"/home/:id/edit\"\r\n            component={CustomerUpdatePage}\r\n          />\r\n\r\n          <Route\r\n            path=\"/home/:id\"\r\n            component={CustomerDetailPage}\r\n          />\r\n\r\n          <Route path=\"/home\">\r\n            <CustomerListPage />\r\n          </Route>\r\n\r\n        </Switch>\r\n      </UserContext.Provider>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default App;\r\n","C:\\Users\\Sebas\\Desktop\\inlamningsuppgift-react\\src\\pages\\LoginPage.jsx",[],"C:\\Users\\Sebas\\Desktop\\inlamningsuppgift-react\\src\\